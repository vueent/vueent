{
  "name": "@vueent/core",
  "description": "Vueent core library",
  "version": "0.4.0",
  "license": "MIT",
  "homepage": "https://vueent.github.io/vueent/guide/core",
  "repository": {
    "type": "git",
    "url": "git@github.com:vueent/vueent.git"
  },
  "bugs": {
    "url": "https://github.com/vueent/vueent/issues"
  },
  "keywords": [
    "vueent",
    "vue",
    "composition-api",
    "reactive",
    "typescript"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "rimraf ./dist && bili",
    "test:clear": "jest --clearCache",
    "test:unit": "jest",
    "test:ext": "jest --collect-coverage",
    "test:debug": "node --inspect node_modules/.bin/jest --watch --runInBand",
    "prepublishOnly": "npm run build && npm run test:unit",
    "lint": "eslint src/**/*.ts,tests/**/*.ts"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^18.11.18",
    "bili": "^5.0.5",
    "eslint": "^7.7.0",
    "jest": "^26.5.0",
    "rimraf": "^3.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "ts-jest": "^26.4.1",
    "typescript": "^4.9.4",
    "vue-demi": "latest"
  },
  "publishConfig": {
    "access": "public"
  }
}
